        "1": {
            "vocab_context_data": {
                "content": "This is the difference between _cardinal_ numbers and _ordinal_ numbers. Cardinal numbers only measure the size of things, without really worrying about where those things all are in relation to one another. Whereas ordinal numbers take the ordering into account.",
                "date_created": "2018-02-19T08:06:51.650317"
            },
            "vocab_entries": [
                {
                    "vocab_entry_data": {
                        "language": "en",
                        "entry": "ordinal number",
                        "pronunciation_spelling": "",
                        "pronunciation_ipa": "",
                        "description": "any of the numbers that express degree, quality, or position in a series, as first, second, and third\r\n\r\n- n\u00famero ordinal",
                        "date_created": "2018-04-12T16:51:54.728292"
                    },
